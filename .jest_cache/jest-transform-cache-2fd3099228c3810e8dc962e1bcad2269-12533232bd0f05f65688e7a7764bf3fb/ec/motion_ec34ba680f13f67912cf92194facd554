// =============================================================================
// TRANSITION HELPERS
// =============================================================================
/**
 * Create a ghost-style transition
 */
const ghostTransition = (delay = 0, dur = exports.MOTION_TOKENS.duration.normal) => ({
    duration: dur,
    delay,
    ease: exports.GHOST_EASE,
});
exports.ghostTransition = ghostTransition;
/**
 * Create a spring transition with ghost physics
 */
const ghostSpringTransition = (springType = 'ghost') => ({
    type: 'spring',
    ...exports.MOTION_TOKENS.spring[springType],
});
exports.ghostSpringTransition = ghostSpringTransition;
/**
 * Viewport animation props (for whileInView)
 */
exports.viewportConfig = {
    once: true,
    margin: exports.MOTION_TOKENS.reveal.margin,
    amount: exports.MOTION_TOKENS.reveal.threshold,
};
// =============================================================================
// REDUCED MOTION HELPER
// =============================================================================
/**
 * Returns empty animation props if user prefers reduced motion
 */
const getMotionProps = (props, prefersReducedMotion) => {
    if (prefersReducedMotion)
        return {};
    return props;
};
exports.getMotionProps = getMotionProps;
