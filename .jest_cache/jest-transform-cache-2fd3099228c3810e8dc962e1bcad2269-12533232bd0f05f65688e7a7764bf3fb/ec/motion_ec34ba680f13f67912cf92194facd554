/**
 * Stagger Container - Parent for staggered children
 * Use for: Lists, grids, card groups
 */
const staggerContainer = (staggerDelay = exports.MOTION_TOKENS.stagger.relaxed) => ({
    hidden: { opacity: 0 },
    visible: {
        opacity: 1,
        transition: {
            staggerChildren: staggerDelay,
            delayChildren: 0.2,
        },
    },
});
exports.staggerContainer = staggerContainer;
